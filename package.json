{
  "name": "debate-cards",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "prestart": "npx prisma migrate deploy",
    "generate": "npx openapi-generator-cli generate",
    "start": "npm run tasks & node ./build/modules/api",
    "start:tasks": "node ./build/tasks",
    "start:api": "node ./build/modules/api",
    "dev": "ts-node-dev --poll --respawn -r tsconfig-paths/register ./src/",
    "dev:api": "ts-node-dev --transpile-only --poll --respawn -r tsconfig-paths/register ./src/modules/api"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.2.1",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@prisma/client": "^3.8.1",
    "apollo-server-express": "^3.10.2",
    "cheerio": "1.0.0-rc.3",
    "class-validator": "^0.13.2",
    "docx": "^6.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.18.1",
    "graphql": "^15.0.0",
    "graphql-fields": "^2.0.3",
    "graphql-query-complexity": "^0.12.0",
    "htmlparser2": "^7.2.0",
    "jsonwebtoken": "^8.5.1",
    "limiter": "^2.1.0",
    "lodash": "^4.17.15",
    "rate-limiter-flexible": "^2.3.11",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "type-graphql": "^1.1.1",
    "typescript-collections": "^1.3.3",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.157",
    "@types/node": "^18.0.5",
    "@types/unzipper": "^0.10.4",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-type-graphql": "^1.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "prisma": "^3.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.2"
  }
}
